#include <stdio.h>
int fib(int x);

int main()
{
    int a;
    printf("enter the valeu of a\n");
    scanf("%d", &a);
    printf("the value of fibonacci %d is %d\n", a, fib(a));
    return 0;
}
int fib(int x)
{
   //printf("calling fibonacci(%d)\n", x);
   int result;
   if (x==1){
        return 0;
    }
    else if (x==2){
        return 1;

    }
    else
    {
    
        result = (x-1) + (x-2);
        return result;
    }
}
