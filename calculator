#include <stdio.h>

int main()
{
    int a, b;
    char c;
    printf("enter opretor value (*,+,-,/)\n");
    scanf("%c", &c);
    printf("enter the value of A\n");
    scanf("%d", &a);
    printf("enter the value of B\n");
    scanf("%d", &b);
    switch (c)
    {
    case '+':
        printf("addition in = %d\n", a + b);
        break;
        case '-':
        printf("sub = %d\n", a-b);
        break;
        case '*':
        printf("multiply = %d\n", a*b);
        break;
        case '/':
        printf("divided = %d\n", a/b);

        defoult:
        printf("invelid");
        break;


    default:
        break;
    }

    return 0;
}
