#include<stdio.h>

int main(){
    int i,j,a[2][2],sum;
    printf("enter four element of ary");
    for ( i = 1; i <=2; i++)
    {
         for ( j = 1; j <=2; i++)
    {
            scanf("%d",&a[i][j]);
    }
    }
    
     printf("your sum =");
     for ( i = 1; i <=2; i++)
     {
        for ( j = 1; i <=2; i++)
        {
            sum+=a[i][j];
        }
     }
     printf("%d",sum);
     
     
    
    

    return 0;
}
